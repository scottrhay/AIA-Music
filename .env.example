# SunoApp Environment Variables
# Copy this file to .env and fill in your actual values

# Secret Keys (generate with: openssl rand -hex 32)
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# Database Configuration
DB_NAME=sunoapp_db
DB_USER=sunoapp_user
DB_PASSWORD=your-database-password-here

# Domain (for CORS and SSL)
DOMAIN=your-domain.com

# CORS Configuration
CORS_ORIGINS=https://your-domain.com,http://localhost:3000

# n8n Webhook Configuration
N8N_WEBHOOK_URL=https://your-n8n-domain.com/webhook/your-webhook-id

# Suno API Configuration
SUNO_API_KEY=your-suno-api-key-here
SUNO_API_URL=https://api.sunoapi.org/api/v1/generate
APP_URL=https://your-domain.com
